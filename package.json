{
  "name": "sharecode-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "start": "node -r module-alias/register ./dist/bin/www",
    "build": "node etc/build",
    "build-linux": "bash etc/build.sh",
    "tsc": "tsc"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "bin"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./bin/www"
  },
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "google-auth-library": "^8.7.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "log4js": "latest",
    "module-alias": "^2.2.2",
    "mongoose": "^6.8.4",
    "morgan": "~1.9.1",
    "zod": "^3.20.2",
    "zod-error": "^1.1.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.2",
    "nodemon": "^2.0.15",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.4.3"
  },
  "_moduleAliases": {
    "@": "./dist/src"
  },
  "eslintIgnore": [
    "src/public/"
  ]
}
